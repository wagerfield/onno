{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "rules": {
    "array-type": [true, "array"],
    "curly": [true, "ignore-same-line"],
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "semicolon": [true, "never"],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  }
}
